<h1>Incidencias del Restaurante</h1>

<%= form_tag incidences_path, :method => 'get', :id => "incidences_search" do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :title => nil %>
  </p>
  <div id="products"><%= render 'incidences' %></div>
<% end %>

<p><%= link_to "New Incidence", new_incidence_path %></p>
