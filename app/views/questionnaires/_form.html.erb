<%= form_for @questionnaire do |f| %>
	<%= f.error_messages %>

	<p>
		<%= f.label :question %><br />
		<%= f.text_area :question, :cols => 30, rows: 2 %>
	</p>

	<p>
		<%= f.label :restaurante %><br />
		<%= f.select :restaurant_id, options_from_collection_for_select(Restaurant.all, :id, :name)%>
	</p>

	<p><%= f.submit %></p>
<% end %>
