<%= form_for @questionnaire do |f| %>
	<%= f.error_messages %>
	<div class="field">
		</br><%= f.label :redactar_pregunta %>
		<%= f.text_area :question, :cols => 30, rows: 2 %>
	</div>
	<%= f.hidden_field :restaurant_id, :value => @current_restaurant %>
	<div class="field">
		<p><%= f.submit 'Crear Cuestionario', :class => 'boton grey'%></p>
	</div>
	</br></br></br></br></br></br></br></br>
<% end %>
