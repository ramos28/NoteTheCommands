<%= form_for @restaurant do |f| %>
      <%= f.error_messages %>
      <p>
            <%= f.label :name %><br />
            <%= f.text_field :name %>
      </p>
      <p>
            <%= f.label :email %><br />
            <%= f.text_field :email %>
      </p>
      <p>
            <%= f.label :phone %><br />
            <%= f.text_field :phone %>
      </p>
      <p>
            <%= f.label :country %><br />
            <%= f.text_field :country %>
      </p>
      <p>
            <%= f.label :postal_code %><br />
            <%= f.text_field :postal_code %>
      </p>
      <p>
            <%= f.label :address %><br />
            <%= f.text_field :address %>
      </p>
      <p><%= f.submit %></p>
<% end %>