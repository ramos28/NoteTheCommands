<div class="title">Bienvenido</div>
<div class ="muestra-tabla-anonimous">
    </br></br><div class="accionTitle">¿Aún no está regristrado? A que esperas para hacerlo..</div>
    </br><%= button_to 'Registrarse', new_registration_path(:user), :method => "get", :class => 'boton grey'  %>

    </br></br></br><div class="accionTitle">Acceda sin Registro</div> 
    </br><%= button_to 'Usuario Invitado', guests_index_path, :method => "get", :class => 'boton grey'  %>
</div>

<div class="muestra-tabla-login">
    </br></br><div class="accionTitle">Acceso Usuario Registrado</div>
    <div id="padre">  
        <div id="hijo8">
            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                <div class="field">
                    </br><%= f.label :correo %>
                    <%= f.email_field :email, autofocus: true %>
                </div>

                <div class="field">
                    <%= f.label :Contraseña %>
                    <%= f.password_field :password, autocomplete: "off" %>
                </div>

                <% if devise_mapping.rememberable? -%>
                    <div class="field">
                        </br><%= f.check_box :remember_me %>  Recordarme
                    </div>
                <% end -%>

                <div class="actions">
                    </br></br><%= f.submit "Iniciar", :class => 'boton grey' %>
                </div>
            <% end %>
            <%= render "devise/shared/links" %>
            </br>
        </div>
    </div>
</div>
